{
    "manifest_version": 2,
    "name": "老机导航",
    "description": "老机自己的导航插件",
    "version": "1.1.0",
    "icons": {
        "16": "assets/logo.png",
        "48": "assets/logo.png",
        "128": "assets/logo.png"
    },
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "assets/logo.png",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "activeTab",
        "tabs",
        "cookies",
        "notifications",
        "storage",
        "<all_urls>"
    ]
}